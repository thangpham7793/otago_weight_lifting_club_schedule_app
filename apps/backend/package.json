    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^29.0.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.6.0",
    "@types/nodemailer": "^6.4.0",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.10",
    "babel-jest": "^26.3.0",
    "browserify": "^17.0.0",
